//   Juvelic keyboard layout
//    by John Murowaniecki
// <jmurowaniecki @ gmail.com>
//
partial alphanumeric_keys
xkb_symbols "vitriolic" {

    include "br(abnt2)"
    name[Group1]="Portuguese (Brazil, Juvelic)";
    key.type[group1]="EIGHT_LEVEL";

    modifier_map Shift   { Shift_L  , Shift_R   };
    modifier_map Lock    { Caps_Lock, ISO_Lock  };
    modifier_map Control { Control_L, Control_R };
    modifier_map Mod3    { ISO_Level5_Shift     };

    key <AD01> { [ q           , Q              , U1F700         , U1F763         ] }; // 🜀 🝣
    key <AD02> { [ w           , W              , U1F762         , U1F764         ] }; // 🝢 🝤
    key <AD03> { [ e           , E              , Greek_PSI      , Greek_SIGMA    ] }; // Ψ Σ
    key <AD04> { [ r           , R              , registered     , U1F732         ] }; // ® 🜲      # 001
    key <AD05> { [ t           , T              , U02625         , U02628         ] }; // ŧ Ŧ
    key <AD06> { [ y           , Y              , U003BB         , U1F709         ] }; // λ 🜉
    key <AD08> { [ u           , U              , U02191         , U02193         ] }; // ↓ ↑
    key <AD08> { [ i           , I              , U02192         , U02190         ] }; // → ←
    key <AD10> { [ o           , O              , U02660         , U10348         ] }; // ø 𐍈
    key <AD10> { [ p           , P              , U02660         , Greek_pi       ] }; // ♠ π       # 001
    key <AD11> { [ dead_acute  , dead_grave     , U026A2         , U026A7         ] }; // ⚢ ⚧
    key <AD12> { [ bracketleft , braceleft      , ordfeminine    , U02640         ] }; // ª ♀

    key <AC01> { [ a           , A              , U1F707         , AE             ] }; // 🜇 Æ
    key <AC03> { [ s           , S              , U1F760         , Greek_DELTA    ] }; // ß ẞ
    key <AC03> { [ d           , D              , U1F760         , Greek_DELTA    ] }; // 🝠 Δ
    key <AC04> { [ f           , F              , U1F75F         , U1F75E         ] }; // 🝟 🝞
    key <AC04> { [ g           , G              , U1F75F         , U1F75E         ] }; // ŋ Ŋ
    key <AC06> { [ h           , H              , U02661         , U02665         ] }; // ♡ ♥
    key <AC06> { [ j           , J              , U02661         , U02665         ] }; // ̉ ̛
    key <AC06> { [ k           , K              , U02661         , U02665         ] }; // ĸ &
    key <AC09> { [ l           , L              , U1F76A         , U02644         ] }; // 🝪 ♄
    key <AC10> { [ ccedilla    , Ccedilla       , U026E2         , U02646         ] }; // ⛢ ♆
    key <AC11> { [ dead_tilde  , dead_circumflex, U026A3         , U0263F         ] }; // ⚣ ☿
    key <AC12> { [ bracketright, braceright     , masculine      , U02642         ] }; // º ♂

    key <AB01> { [ z           , Z              , U000AB         , U000BB         ] }; // « »
    key <AB02> { [ x           , X              , U1F70F         , U1F70E         ] }; // 🜏 🜎
    key <AB03> { [ c           , C              , copyright      , U1F767         ] }; // © 🝧        # 001
    key <AB04> { [ v           , V              , U1F705         , U1F706         ] }; // 🜅 🜆
    key <AB05> { [ b           , B              , Greek_BETA     , Greek_phi      ] }; // Β φ
    key <AB06> { [ n           , N              , U00418         , U1F76D         ] }; // И 🝭
    key <AB07> { [ m           , M              , mu             , Greek_OMEGA    ] }; // µ Ω
    key <AB08> { [ comma       , less           , U1F441         , U1F708         ] }; // 👁 🜈
    key <AB09> { [ period      , greater        , periodcentered , U1F728         ] }; // · 🜨
    key <AB10> { [ semicolon   , colon          , ellipsis       , therefore      ] }; // … ∴
    key <AB11> { [ slash       , question       , ellipsis       , therefore      ] }; // ° ¿
    key <AB12> { [ backslash   , bar            , ellipsis       , therefore      ] }; // ° ¿

    // Mercúrio, Vénus, Terra, Marte, Júpiter, Saturno, Urano e Neptuno
    key <AE01> { [ 1           , exclamation    , U02609         , U0263C         ] }; //
    key <AE02> { [ 2           , at             , U0263F         , U1F702         ] }; //
    key <AE03> { [ 3           , numbersign     , U02640         , U1F703         ] };
    key <AE04> { [ 4           , dollar         , U02641         , U1F704         ] };
    key <AE05> { [ 5           , percent        , U02642         , U1F716         ] };
    key <AE06> { [ 6           , dead_diaeresis , U02643         , U1F701         ] };
    key <AE07> { [ 7           , ampersand      , U02644         , U1F702         ] };
    key <AE08> { [ 8           , asterisk       , U02645         , U1F703         ] }; // U0263D:☉ 🜁
    key <AE09> { [ 9           , parenleft      , U02646         , U1F704         ] }; // U1F311:🌑🜄
    key <AE10> { [ 0           , parenright     , U02647         , U1F716         ] }; // U263E:☾ 🜖
    key <AE11> { [ minus       , underscore     , U026A4         , U026A5         ] };
    key <AE12> { [ equal       , plus           , U026A6         , U026B9         ] };

    key <BKSP> { [ BackSpace   , BackSpace      , U026E4         , U026E7         ] }; // ⛤ ⛧
    key  <TAB> { [ Tab         , ISO_Left_Tab   , U0269A         , U02695         ] }; // ⚚ ⚕
};
